<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<launch>
	<arg name="freq" default="200" />

	<node pkg="republisher" type="republisher_node" name="insole_republisher_right" >
		<param name="rate" 				value="$(arg freq)"/>
		<param name="input_queue_length" 		value="100"/>
		<param name="maximum_queue_length" 		value="1000"/>
		<param name="rate" 				value="$(arg freq)"/>
		<remap from="/insole_republisher_right/input" 	to="/right/insole"/>
		<remap from="/insole_republisher_right/output" 	to="/right/insole_fixed_freq"/>
	</node>
	<node pkg="republisher" type="republisher_node" name="insole_republisher_left" >
		<param name="rate" 				value="$(arg freq)"/>
		<param name="input_queue_length" 		value="100"/>
		<param name="maximum_queue_length" 		value="1000"/>
		<param name="rate" 				value="$(arg freq)"/>
		<remap from="/insole_republisher_left/input" 	to="/left/insole"/>
		<remap from="/insole_republisher_left/output" 	to="/left/insole_fixed_freq"/>
	</node>

</launch>


